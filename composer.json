{
    "name": "john-jun/container",
	"description": "A simple Container is implemented based on psr-11",
    "keywords": ["Air", "Container", "DI", "IOC"],
    "license": "MIT",
    "authors": [
        {
            "name": "John",
            "email": "53950451@qq.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "psr/container": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Air\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Air\\Container\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "ext-redis": "*"
    },
	"extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
